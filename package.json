    "copy:bootstrap": "mkdir -p libs/bootstrap; mv -t libs/bootstrap node_modules/bootstrap/dist/css/bootstrap.*css",
    "copy:leaflet": "mkdir -p libs/leaflet;mv -t libs/leaflet node_modules/leaflet/dist/leaflet*",
    "copy:knockout": "mkdir -p libs/knockout; mv -t libs/knockout node_modules/knockout/build/output/knockout*",
    "postinstall": "npm run copy"
  },
